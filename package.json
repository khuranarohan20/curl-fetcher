{
  "name": "curl-fetcher",
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently -k \"cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron .\" \"vite\"",
    "build": "vite build && electron-builder",
    "start": "electron ."
  },
  "main": "electron/main.js",
  "build": {
    "appId": "com.curlfetcher.app",
    "mac": {
      "target": {
